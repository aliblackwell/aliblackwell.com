<nav class="child-nav" aria-labelledby="nav-title">
  <h1 id="nav-title">{{navParentName}}</h1>
  {% set navPages = collections.all | eleventyNavigation(navParentName) %}
  <ul>
    {% for nav in options.childNavs %}
      {% if nav.name == navParentName%}
        <li{% if nav.path == page.url %} class="my-active-class" {% endif %}><a href="{{nav.path}}">{{nav.label}}</a></li>
      {% endif %}
    {% endfor %}
    {%- for entry in navPages %}
      <li{% if entry.url == page.url %} class="my-active-class" {% endif %}>
        <a href="{{ entry.url | url }}">{{ entry.title }}</a>
      </li>
    {%- endfor %}
  </ul>

</nav>