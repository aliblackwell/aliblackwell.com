<link rel="icon" type="image/png" href="{{meta.favicon}}">

<meta name="description" content="{{meta.siteSlogan}}"/>
<meta property="og:title" content="{{meta.siteTitle}}">
<meta property="og:site_name" content="{{meta.siteTitle}}">
<meta property="og:url" content="{{meta.siteUrl}}">
<meta property="og:description" content="{{meta.siteSlogan}}"/>
<meta property="og:type" content="website">
<meta property="og:image" content="{{meta.siteUrl}}/img/landscape-banner.png"/>
<meta property="og:image:width" content="1024"/>
<meta property="og:image:height" content="512"/>
